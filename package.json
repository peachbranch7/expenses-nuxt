{
  "name": "expenses-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"**/*.{vue,scss,css}\" --ignore-path .gitignore",
    "lint:style:fix": "stylelint \"**/*.{vue,scss,css}\" --ignore-path .gitignore --fix",
    "prepare": "husky install",
    "lint": "yarn lint:js && yarn lint:style",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.30.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/firebase": "^8.1.1",
    "core-js": "^3.15.1",
    "dayjs": "^1.10.7",
    "firebase": "^9.6.1",
    "firebase-tools": "^10.0.1",
    "nuxt": "^2.15.7",
    "sass": "^1.43.4",
    "sass-loader": "^10.1.1",
    "vee-validate": "3.x",
    "vuejs-datepicker": "^1.6.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-rscss": "^0.4.0",
    "vuex-module-decorators": "^2.0.0"
  }
}
